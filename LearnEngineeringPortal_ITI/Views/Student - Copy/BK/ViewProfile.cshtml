        display: none !important;
    }

    .navbar {
        padding: 0rem 1rem;
    }

    .mn-hd {
        color: #fff;
        padding-left: 2%;
        margin: 0;
        font-size: 20px;
    }

    .mn-hd {
        color: #fff;
