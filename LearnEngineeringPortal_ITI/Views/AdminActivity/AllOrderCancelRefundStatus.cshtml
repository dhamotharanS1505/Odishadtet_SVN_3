            function (data) {
                $.each(data, function (index, value) {
                    if (index == 0) {

                        $("#orders_shippment").tmpl(value).appendTo("#purchased");
                    }
                    $("#purchasedorder_details").tmpl(value).appendTo("#purchased_details");

                });
            }
        );
    }

