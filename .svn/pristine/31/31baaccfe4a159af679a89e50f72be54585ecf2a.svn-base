@font-face {
  font-family: "Flaticon";
  src: url("../fonts_v2/Flaticon.eot");
  src: url("../fonts_v2/Flaticon.eot?#iefix") format("embedded-opentype"),
       url("../fonts_v2/Flaticon.woff") format("woff"),
       url("../fonts_v2/Flaticon.ttf") format("truetype"),
       url("../fonts_v2/Flaticon.svg#Flaticon") format("svg");
  font-weight: normal;
  font-style: normal;
}

@media screen and (-webkit-min-device-pixel-ratio:0) {
  @font-face {
    font-family: "Flaticon";
    src: url("../fonts_v2/Flaticon.svg#Flaticon") format("svg");
  }
}

[class^="flaticon-"]:before, [class*=" flaticon-"]:before,
[class^="flaticon-"]:after, [class*=" flaticon-"]:after {   
  font-family: Flaticon;
font-style: normal;
}

.flaticon-educational-book:before { content: "\f100"; }
.flaticon-light-bulb:before { content: "\f101"; }
.flaticon-manual-book:before { content: "\f102"; }
.flaticon-open-magazine:before { content: "\f103"; }
.flaticon-pencil-edit-button:before { content: "\f104"; }
.flaticon-question:before { content: "\f105"; }
.flaticon-reading:before { content: "\f106"; }
.flaticon-technology:before { content: "\f107"; }