    <link href="../Scripts/Alert/alert_box.css" rel="stylesheet" />
    <script src="../Scripts/jquery-1.11.1.min.js"></script>
    <script src="../Scripts/Alert/sweetalert-dev.js"></script>
    <script type="text/javascript">

        $(function () {
            $('#mobileid').keypress(function (e) {
                if (e.keyCode == '13') {
                    $("#submit").trigger("click");
                }
            });


            $("#submit").click(function () {
                if (($("#mobileid").val() == "") || $("#mobileid").val().length != 10) {

                    ShowAlert("Alert", "Enter Valid Mobile Number.", "warning");
                }
                else {
                    $.ajax({
                        url: "/APIService/APIAdminActivity/LoadOtpcode?Mobile=" + $("#mobileid").val(),
                        type: 'GET',
                        async: false,
                        success: function (Rdata) {
                            if (Rdata == "0") {
                                ShowAlert("Alert", "Mobile Number Not Registered With Us.", "warning");
                                $('#GetserverDataLiveSub').html("");
                            }
                            else {
                                $('#GetserverDataLiveSub').html(Rdata);
                            }
                        }
                    });
                }
            });


            $('#clear').click(function () {
                $("#mobileid").val('');
                $('#GetserverDataLiveSub').html("");
            });
        });
    </script>
