
@{
    ViewBag.Title = "ChennalPartnerReport";
    Layout = "~/Views/Shared/_LearnEnggAdminLayout.cshtml";
}

<style>
    .btn-group button b {
        bottom: 0;
        display: inline-block;
        left: 30% !important;
        position: fixed;
        top: 78% !important;
    }

    .big_inp button b {
        bottom: 0;
        display: inline-block;
        left: 95% !important;
        position: fixed;
        top: 128px !important;
    }
    .big_inp_tm button b {
        bottom: 0;
