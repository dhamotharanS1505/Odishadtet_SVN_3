<article class="col-sm-12">
    <div class="in_hd_tt row"><strong class="fa fa-file-text"></strong><span>Chennal Partner Report</span></div>
</article>

<article class="col-sm-12">
    <div class="sub_wp_bx row">
        <h5 id="DynamicFilteredData"></h5>
        <div style="float:right">
            <input type="button" class="fil_in_btn" value="Filter" id="btnFilter" data-toggle="modal" data-target="#FilterModel" rel="btn" />
            <input type="button" class="fil_in_btn" value="Clear Filter" id="refreshFilter" />
        </div>
    </div>
</article>
<article class="col-sm-12">

    <section class="sub_wrp row">


        <table id="tblChennalPartner" class="table table-striped table-bordered" cellspacing="0" width="100%">

            <thead>
                <tr>
                    <th>
                        S.No
                    </th>
                    <th>
                        University
                    </th>
                    <th>
                        Channel Partner
                    </th>

                    <th>
                        Mobile No
                    </th>
                    <th>
                        Assigned Colleges
                    </th>
                    <th>
                        Registered Colleges
                    </th>
                    <th>
                        Registered Student
                    </th>
                    <th>
                        Registered Staff
                    </th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>

    </section>

    <section class="sub_wrp row" id="chanlpartnershow" style="display:none;">
        <div class="col-sm-12" id="dvReadHistry">


            <div class="loader">
                <center>
                    <img src="../Content/img/pre_loader.gif" />
                </center>
            </div>

            <table id="tblChennalPartnerCollegeWise" class="table table-striped table-bordered" cellspacing="0">

                <thead>
                    <tr>
                        <th>
                            S.No
                        </th>

                        <th>
                            College Name
                        </th>
                        <th>Team</th>
                        <th>
                            Registered Student
                        </th>
                        <th>
                            Registered Staff
                        </th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>


            @*    <ul>
                     <li>
                <input type="button" id="btnChannelPartner" value="Export ChannelPartner Report" />
                     </li>
                     </ul>*@
        </div>
    </section>



</article>
<div class="modal fade" id="channelPartner" role="dialog">
    <div class="modal-dialog custom_pop pop_up_width">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button data-dismiss="modal" class="close" type="button">
                    ×
                </button>
                <h4 id="UserrName" class="pop_hed"></h4>
                @*<p class="small_txt">View Details</p>*@
            </div>
            <div class="modal-body">
                <section class="pop_head full_width">

                    <div class="row down_panel" id="userSubjectDetails">
                        <div class="loader">
                            <center>
                                <img src="../Content/img/pre_loader.gif" />
                            </center>
                        </div>

                        <table id="tblchannelPartnerReport" class="table table-striped table-bordered" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th style="width:1px !important">
                                        S.No
                                    </th>
                                    <th>
                                        College Name
                                    </th>
                                    <th>Team</th>
                                    <th>
                                        Student Name
                                    </th>
                                    <th>
                                        Mobile
                                    </th>
                                    <th>
                                        Email
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>

                    </div>

                </section>
            </div>
        </div>
    </div>
</div>
