<div class="modal fade" id="CPR_TeamWise" role="dialog">
    <div class="modal-dialog custom_pop pop_up_width">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button data-dismiss="modal" class="close" type="button">
                    ×
                </button>
                <h4 id="Cus_Name" class="pop_hed"></h4>
                @*<p class="small_txt">View Details</p>*@
            </div>
            <div class="modal-body">
                <section class="pop_head full_width">

                    <div class="row down_panel" id="userSubjectDetails">
                        <div class="loader">
