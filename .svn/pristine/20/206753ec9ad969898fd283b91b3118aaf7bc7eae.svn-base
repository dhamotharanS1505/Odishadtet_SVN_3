            <div class="col-md-12" style="box-shadow: 0 1px 2px 2px #e6ecf1; margin-top:2%; background-color: #fff; width: 1100px; height: 370px;" id="MonthBarChartdiv">
                @*<img src="../files/images/chart 2-01.png" style="width:100%;">*@
               
            </div>
        </div>
