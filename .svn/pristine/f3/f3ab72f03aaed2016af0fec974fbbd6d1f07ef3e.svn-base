    ViewBag.Title = "DataAnalyzeReport";
    Layout = "~/Views/Shared/_LearnEnggAdminLayout.cshtml";
}

<style>
    .wrapper {
        background: none;
    }

    .btn {
        min-width: 85px;
        font-size: 14px;
        font-weight: 500;
    }

    .modal-header {
        border-bottom: none !important;
        padding: 15px;
    }

    .md_cnt_hgt {
        height: 420px;
    }

    #EndDate_reg {
        margin-top: 0 !important;
    }

    #FilterModel .modal-dialog {
        width: 56% !important;
    }

    .mrg-btm {
        margin-bottom: 8px;
    }

    .fl_in_det li.date_p input {
